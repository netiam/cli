  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netiam/cli/issues"
  },
  "homepage": "https://github.com/netiam/cli",
  "engines": {
    "node": ">=v0.12",
    "iojs": ">=v2",
    "npm": ">=2.1.12"
  }
}
