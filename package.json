{
  "name": "netiam-cli",
  "description": "netiam CLI tools",
  "main": "index.js",
  "bin": {
    "netiam": "./bin/netiam"
  },
  "preferGlobal": true,
  "scripts": {
    "build": "./bin/build",
    "clean": "./bin/clean",
    "lint": "./bin/lint",
    "start": "npm run watch",
    "test": "npm run test:unit",
    "test:unit": "mocha",
    "test:coverage": "istanbul cover _mocha --report lcovonly -- -R spec",
    "watch": "./bin/watch",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "async": "1.5.0",
    "babel-runtime": "6.0.8",
    "chalk": "1.1.1",
    "debug": "2.2.0",
    "dot": "1.0.3",
    "exit": "0.1.2",
    "git-scope-config": "0.0.0",
    "glob": "5.0.15",
    "inquirer": "1.0.0",
    "is-git-repo": "0.1.2",
    "lodash": "4.13.1",
    "mkdirp": "0.5.1",
    "nopt": "3.0.4",
    "pleasant-progress": "1.1.0",
    "readline2": "1.0.1",
    "recast": "0.11.0",
    "resolve": "1.1.7",
    "rimraf": "2.4.3",
    "through": "2.3.8"
  },
  "devDependencies": {
    "babel": "6.0.15",
    "codeclimate-test-reporter": "0.3.1",
    "eslint": "2.7.0",
    "istanbul": "0.4.2",
    "mocha": "2.4.5",
    "semantic-release": "4.3.5",
    "should": "8.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/netiam/cli.git"
  },
  "keywords": [
    "CLI",
    "tools"
  ],
